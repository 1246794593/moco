[
    {
        "request": {
            "uri": "/template"
        },
        "response": {
            "text": {
                "template" : "${req.method}"
            }
        }
    },
    {
        "request": {
            "uri": "/file_template"
        },
        "response": {
            "file": {
                "template" : "src/test/resources/template.foo"
            }
        }
    },
    {
        "request": {
            "uri": "/path_resource_template"
        },
        "response": {
            "path_resource": {
                "template" : "template.foo"
            }
        }
    },
    {
        "request": {
            "uri": "/version_template"
        },
        "response": {
            "version": {
                "template" : "${req.version}"
            }
        }
    }
]